{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ValueWithUnits Registry",
  "description": "Schema för enhetsregister för ValueWithUnits-systemet",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["unit", "dimension"],
    "properties": {
      "unit": {
        "type": "string",
        "description": "Enhetens namn"
      },
      "dimension": {
        "type": "array",
        "items": {
          "type": "integer",
          "minimum": -128,
          "maximum": 127
        },
        "minItems": 8,
        "maxItems": 8,
        "description": "Dimensionsvektor [längd, tid, massa, ström, temperatur, substansmängd, ljusstyrka, ortogonalitet]"
      },
      "symbol": {
        "type": "string",
        "description": "Symbol för enheten (om utelämnad används automatiskt SI-prefix)"
      },
      "factor": {
        "type": "number",
        "exclusiveMinimum": 0,
        "description": "Omvandlingsfaktor från SI-enheter (måste anges om symbol anges)"
      }
    },
    "dependencies": {
      "symbol": ["factor"],
      "factor": ["symbol"]
    }
  }
}
